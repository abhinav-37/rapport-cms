<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <title>Engitech</title>
    <link rel="shortcut icon" type="image/x-icon" href="/images/favicon.png" />
    <link rel="stylesheet" href="/css/bootstrap.min.css" />
    <link rel="stylesheet" href="/css/font-awesome.min.css" />
    <link rel="stylesheet" href="/css/flaticon.css" />
    <link rel="stylesheet" href="/css/owl.carousel.min.css" />
    <link rel="stylesheet" href="/css/owl.theme.css" />
    <link rel="stylesheet" href="/css/magnific-popup.css" />

    <link rel="stylesheet" href="/css/style.css" />
    <link rel="stylesheet" href="/css/royal-preload.css" />
    <style>
        .byline {
            color: black;
        }

        .entry-title a {
            font-size: 20px;
            font-weight: 500;
        }

        .rates-wages {
            display: flex;
            justify-content: space-around;
        }

        tr td,
        tr th {
            text-align: center !important;
        }
    </style>
</head>

<body class="royal_preloader">
    <div id="page" class="site">
        <%- include("partials/navbar") %>

        <div id="content" class="site-content">
            <div class="page-header flex-middle">
                <div class="container">
                    <div class="inner flex-middle">
                        <h1 class="page-title">Minimum Wages</h1>
                        <ul id="breadcrumbs" class="breadcrumbs none-style">
                            <li><a href="/">Home</a></li>
                            <li class="active">Minimum Wages</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <table class="table container">
            <thead class="thead-dark">
                <tr class="table-head">
                    <th scope="col">Schedule of Employment</th>
                    <th scope="col">Category of workmen/Employees</th>
                    <th scope="col">Minimum rates of wages in Rupees <span class="rates-wages"><span>Per
                                Month</span><span>Per
                                Day</span></span></th>
                    <th scope="col"> File </th>
                </tr>
            </thead>
            <tbody>
                <% wages.forEach(function(e){ %>
                <tr>
                    <td scope="row"><%= e.schedule %> </td>
                    <td> <%= e.category %> </td>
                    <td class="rates-wages"><span><%= e.minimumRates.ratesPerMonth %> </span>
                        <span><%= e.minimumRates.ratesPerDay %> </span></td>
                    <td><a target="_blank" href="/uploads/<%-e.filename%> "><i class="fas fa-file-pdf"></i></a></td>
                </tr>
                <% }) %>
            </tbody>
        </table>




    </div><!-- #page -->
    <%- include("partials/footer") %>
    <a id="back-to-top" href="#" class="show"><i class="flaticon-up-arrow"></i></a>
    <!-- jQuery -->
    <script src="js/jquery.min.js"></script>
    <script src="js/jquery.magnific-popup.min.js"></script>
    <script src="js/jquery.isotope.min.js"></script>
    <script src="js/owl.carousel.min.js"></script>
    <script src="js/easypiechart.min.js"></script>
    <script src="js/jquery.countdown.min.js"></script>
    <script src="js/scripts.js"></script>
    <script src="js/header-mobile.js"></script>
    <script src="js/royal_preloader.min.js"></script>
    <script>
        window.jQuery = window.$ = jQuery;
        (function ($) {
            "use strict";
            //Preloader
            Royal_Preloader.config({
                mode: 'logo',
                logo: '/images/logo.svg',
                logo_size: [160, 75],
                showProgress: true,
                showPercentage: true,
                text_colour: '#000000',
                background: '#ffffff'
            });
        })(jQuery);
    </script>
</body>

</html>