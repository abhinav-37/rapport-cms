<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <title>Customer Dashboard</title>
    <link rel="shortcut icon" type="image/x-icon" href="/images/favicon.png" />
    <link rel="stylesheet" href="/css/bootstrap.min.css" />
    <link rel="stylesheet" href="/css/font-awesome.min.css" />
    <link rel="stylesheet" href="/css/flaticon.css" />
    <link rel="stylesheet" href="/css/owl.carousel.min.css" />
    <link rel="stylesheet" href="/css/owl.theme.css" />
    <link rel="stylesheet" href="/css/magnific-popup.css" />

    <link rel="stylesheet" href="/css/style.css" />
    <link rel="stylesheet" href="/css/royal-preload.css" />
    <style>
        .byline {
            color: black;
        }

        .entry-title a {
            font-size: 20px;
            font-weight: 500;
        }
    </style>
</head>

<body class="royal_preloader">
    <div id="page" class="site">
        <%- include("partials/navbar") %>

        <div id="content" class="site-content">
            <div style="background-image:url(https://images.unsplash.com/photo-1454165804606-c3d57bc86b40?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=1350&q=80)"
                class="page-header flex-middle">
                <div class="container">
                    <div class="inner flex-middle">
                        <h1 class="page-title">Dashboard</h1>

                    </div>
                </div>
            </div>
        </div>

        <div class="entry-content">
            <div class="container">
                <div class="blog-grid pgrid">
                    <div class="row">
                        <% orders.forEach(function(u){ %>
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <article class="post-box blog-item">
                                <div class="post-inner">
                                    <div class="inner-post">
                                        <div class="entry-header">
                                            <div class="entry-meta">
                                                <div class="post-cat">
                                                    <div class="row">
                                                        <span class="byline col-lg-4 col-md-4 col-sm-4">
                                                            <p rel="category tag"><span style="color: #43baff;">Name of
                                                                    Service:
                                                                </span>
                                                                <%= u.nameOfService  %>
                                                            </p>
                                                        </span>
                                                        <span class="byline col-lg-4 col-md-4 col-sm-4 ">
                                                            <p> <span style="color: #43baff;">Service Type: </span>
                                                                <%= u.serviceType %>
                                                            </p>
                                                        </span>
                                                        <span class="byline col-lg-4 col-md-4 col-sm-4">
                                                            <p>
                                                                <span style="color: #43baff;">
                                                                    Status:
                                                                </span>
                                                                <span
                                                                    class="btn btn-primary"><%= u.statusOfWork%></span>
                                                            </p>
                                                        </span>
                                                    </div>
                                                </div>



                                            </div><!-- .entry-meta -->


                                        </div><!-- .entry-header -->


                                    </div>
                                </div>
                            </article>
                        </div>
                        <% }) %>


                    </div>
                </div>

            </div>
        </div>

        <%- include("partials/footer") %>
    </div><!-- #page -->
    <a id="back-to-top" href="#" class="show"><i class="flaticon-up-arrow"></i></a>
    <!-- jQuery -->
    <script src="/js/jquery.min.js"></script>
    <script src="/js/jquery.magnific-popup.min.js"></script>
    <script src="/js/jquery.isotope.min.js"></script>
    <script src="/js/owl.carousel.min.js"></script>
    <script src="/js/easypiechart.min.js"></script>
    <script src="/js/jquery.countdown.min.js"></script>
    <script src="/js/scripts.js"></script>
    <script src="/js/header-mobile.js"></script>
    <script src="/js/royal_preloader.min.js"></script>
    <script>
        window.jQuery = window.$ = jQuery;
        (function ($) {
            "use strict";
            //Preloader
            Royal_Preloader.config({
                mode: 'logo',
                logo: '/images/logo.svg',
                logo_size: [160, 75],
                showProgress: true,
                showPercentage: true,
                text_colour: '#000000',
                background: '#ffffff'
            });
        })(jQuery);
    </script>
</body>

</html>